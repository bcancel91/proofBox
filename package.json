    "algoliasearch": "^4.2.0",
    "aws-sdk": "^2.665.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.10",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.3",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "react-instantsearch-dom": "^6.4.0",
    "uuid": "^7.0.3"
  }
}
